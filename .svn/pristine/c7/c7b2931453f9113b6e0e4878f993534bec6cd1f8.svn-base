/* forward declarations */

void sysMpc8540RegInit (UINT32 *);
void sysMpc8540StaticInit (void);

/******************************************************************************
*
* sysMpc8540RegInit - Initialize MPC8540 registers
*
* This function statically initializes specific MPC8540 registers specified
* by a table pointed at by the input parameter.
*
* RETURNS: NA
*/

void sysMpc8540RegInit
    (
    UINT32 *p           /* Pointer to initialization table */
    )
    {
    while (*p != END_INIT_MPC8540_REG)
        {
        CCSR_WRITE32 (CCSBAR, INTERNAL_REG_OFFSET (p), INTERNAL_REG_VALUE (p));
        p += MPC8540_INTERNAL_INIT_SIZE;
        }
    }

/******************************************************************************
*
* sysMpc8540StaticInit - Static init of mpc8540 integrated register set.
*
* This routine performs the static initialization of the mpc8540 register set.
* 
* RETURNS: NA
*/

void sysMpc8540StaticInit (void)
    {
    UINT32 *p = mpc8540InitTbl;
    sysMpc8540RegInit (p);
    }
